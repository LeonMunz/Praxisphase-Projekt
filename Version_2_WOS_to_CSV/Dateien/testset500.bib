@article{ ISI:000513180300002,
Author = {Rokohl, A. C. and Kopecky, A. and Guo, Y. and Kakkassery, V and Mor, J.
   M. and Loreck, N. and Koch, K. R. and Heindl, L. M.},
Title = {{Surgical resection with ophthalmoplastic reconstruction Gold standard in
   periocular basal cell carcinoma}},
Journal = {{OPHTHALMOLOGE}},
Year = {{2020}},
Volume = {{117}},
Number = {{2, SI}},
Pages = {{95-105}},
Month = {{FEB}},
Abstract = {{The gold standard for the treatment of periocular basal cell carcinoma
   is surgical resection followed by ophthalmoplastic reconstruction. The
   highest priority in most cases is the complete histopathologically
   controlled tumor excision. The histopathological preparation can be
   carried out in two stages by rapid overnight embedding or
   intraoperatively by a rapid frozen section procedure. A variety of
   reconstruction methods enable a customized and in most cases also a
   cosmetically and functionally attractive defect coverage.
   Postoperatively, a regularly performed tumor aftercare is essential.}},
