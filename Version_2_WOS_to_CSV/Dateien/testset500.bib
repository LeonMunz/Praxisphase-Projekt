@article{ ISI:000515167400001,
Author = {Soderholm, Joshua S. and Kumjian, Matthew R. and McCarthy, Nicholas and
   Maldonado, Paula and Wang, Minzheng},
Title = {{Quantifying hail size distributions from the sky - application of drone
   aerial photogrammetry}},
Journal = {{ATMOSPHERIC MEASUREMENT TECHNIQUES}},
Year = {{2020}},
Volume = {{13}},
Number = {{2}},
Pages = {{747-754}},
Month = {{FEB 17}},
Abstract = {{A new technique, named ``HailPixel{''}, is introduced for measuring the
   maximum dimension and intermediate dimension of hailstones from aerial
   imagery. The photogrammetry procedure applies a convolutional neural
   network for robust detection of hailstones against complex backgrounds
   and an edge detection method for measuring the shape of identified
   hailstones. This semi-automated technique is capable of measuring many
   thousands of hailstones within a single survey, which is several orders
   of magnitude larger (e.g. 10000 or more hailstones) than population
   sizes from existing sensors (e.g. a hail pad). Comparison with a
   co-located hail pad for an Argentinian hailstorm event during the
   RELAMPAGO project demonstrates the larger population size of the
   HailPixel survey significantly improves the shape and tails of the
   observed hail size distribution. When hail fall is sparse, such as
   during large and giant hail events, the large survey area of this
   technique is especially advantageous for resolving the hail size
   distribution.}},
