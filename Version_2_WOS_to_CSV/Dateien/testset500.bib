@article{ ISI:000510315400005,
Author = {Moree, Pieter and Eddin, Sumaia Saad and Sedunova, Alisa and Suzuki,
   Yuta},
Title = {{Jordan totient quotients}},
Journal = {{JOURNAL OF NUMBER THEORY}},
Year = {{2020}},
Volume = {{209}},
Pages = {{147-166}},
Month = {{APR}},
Abstract = {{The Jordan totient J(k)(n) can be defined by J(k)(n) = n(k) Pi(p
   vertical bar n)(1-p(-k)). In this paper, we study the average behavior
   of fractions P/Q of two products P and Q of Jordan totients, which we
   call Jordan totient quotients. To this end, we describe two general and
   ready-to-use methods that allow one to deal with a larger class of
   totient functions. The first one is elementary and the second one uses
   an advanced method due to Balakrishnan and Petermann. As an application,
   we determine the average behavior of the Jordan totient quotient, the
   kth normalized derivative of the nth cyclotomic polynomial Phi(n)(z) at
   z = 1, the second normalized derivative of the nth cyclotomic polynomial
   Phi(n)(z) at z = -1, and the average order of the Schwarzian derivative
   of Phi(n)(z) at z = 1. (C) 2019 Elsevier Inc. All rights reserved.}},
