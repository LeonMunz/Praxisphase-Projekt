@article{ ISI:000513128100001,
Author = {Heratizadeh, A. and Haufe, E. and Stoelzl, D. and Abraham, S. and
   Heinrich, L. and Kleinheinz, A. and Wollenberg, A. and Weisshaar, E. and
   Augustin, M. and Wiemers, F. and Zink, A. and von Kiedrowski, R. and
