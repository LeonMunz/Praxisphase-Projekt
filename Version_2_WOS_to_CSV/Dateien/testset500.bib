@article{ ISI:000508018500023,
Author = {Le, Hoai and Haidenbauer, Johann and Meissner, Ulf-G. and Nogga, Andreas},
Title = {{Implications of an increased Lambda-separation energy of the hypertriton}},
Journal = {{PHYSICS LETTERS B}},
Year = {{2020}},
Volume = {{801}},
Month = {{FEB 10}},
Abstract = {{Stimulated by recent indications that the binding energy of the
   hypertriton could be significantly larger than so far assumed,
   requirements of a more strongly bound H-3(Lambda) state for the
   hyperon-nucleon interaction and consequences for the binding energies of
   A = 4, 5 and 7 hypernuclei are investigated. As basis, a YN potential
   derived at next-to-leading order in chiral effective field theory is
   employed. Faddeev and Yakubovsky equations are solved to obtain the
   corresponding 3- and 4-body binding energies, respectively, and the
   Jacobi no-core shell model is used for He-5(Lambda) and Li-7(Lambda). It
   is found that the spin-singlet Lambda p interaction would have to be
   much more attractive which can be, however, accommodated within the
   bounds set by the available Lambda p scattering data. The binding
   energies of the He-4(Lambda) hypernucleus are predicted to be closer to
   the empirical values than for YN interactions that produce a more weakly
   bound H-3(Lambda). The quality of the description of the separation
   energy and excitation spectrum for Li-7(Lambda) remains essentially
   unchanged. (C) 2020 The Authors. Published by Elsevier B.V.}},
