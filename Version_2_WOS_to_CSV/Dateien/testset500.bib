@article{ ISI:000509421300001,
Author = {Oguzoglu, Umut and Polidano, Cain and Ha Vu},
Title = {{Impacts from Delaying Access to Retirement Benefits on Welfare Receipt
   and Expenditure: Evidence from a Natural Experiment}},
Journal = {{ECONOMIC RECORD}},
Abstract = {{Governments are responding to fiscal pressures associated with ageing
   populations by increasing the eligibility age for publicly funded
   retirement benefits. However, recent studies show large resulting
   increases in the receipt of disability and unemployment benefits, which
   raises concern that welfare savings are offset by increased inflows into
   alternative payments. Using administrative data to examine the impacts
   of female eligibility age increases in Australia, we find little
   evidence of this. Instead, most of the increase is because the delay
   mechanically extends the receipt time of people already on alternative
   payments. The implication is that fiscal savings are not jeopardised by
   welfare substitution.}},
