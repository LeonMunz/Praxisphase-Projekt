@article{ ISI:000511562700008,
Author = {Flicker, Yuval Z.},
Title = {{Affine Quantum Super Schur-Weyl Duality}},
Journal = {{ALGEBRAS AND REPRESENTATION THEORY}},
Year = {{2020}},
Volume = {{23}},
Number = {{1}},
Pages = {{135-167}},
Month = {{FEB}},
Abstract = {{The Schur-Weyl duality, which started as the study of the commuting
   actions of the symmetric group Sd and GL(n, C) on V. d where V = Cn, was
   extended by Drinfeld and Jimbo to the context of the finite
   Iwahori-Hecke algebra Hd(q2) and quantum algebras Uq (gl(n)), on using
   universal R-matrices, which solve the Yang-Baxter equation. There were
   two extensions of this duality in the Hecke-quantum case: to the affine
   case, by Chari and Pressley, and to the super case, by Moon and by
   Mitsuhashi. We complete this chain of works by completing the cube,
   dealing with the general affine super case, relating the commuting
   actions of the affine Iwahori-Hecke algebra Ha d (q2) and of the affine
   quantum Lie superalgebra Us q,a(sl( m, n)) using the presentation by
   Yamane in terms of generators and relations, acting on the dth tensor
   power of the superspace V = Cm+n. Thus we construct a functor and show
   it is an equivalence of categories of Ha d (q2) and Us q,a(sl( m,
   n))-modules when d < n = m + n.}},
