@article{ ISI:000514686000002,
Author = {Petrovici, A. and Mare, A. S.},
Title = {{Triple shape coexistence and beta decay of Y-96 to Zr-96}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{2020}},
Volume = {{101}},
Number = {{2}},
Month = {{FEB 19}},
Abstract = {{The effects of shape coexistence in Y-96 and Zr-96 on the beta(-)
   first-forbidden decay of the 0(-) ground state and Gamow-Teller decay of
   the 8(+) isomer of Y-96 to Zr-96 were studied within the
   beyond-mean-field complex excited Vampir model. The structure of the
   parent and daughter states and the beta-decay properties have been
   investigated using an effective interaction derived from a G matrix
   based on the charge-dependent Bonn CD potential and a large model space.
   The influence of shape coexistence and mixing on the properties of the
   states involved in the investigated allowed and first-forbidden beta(-)
   decays is discussed and comparison to the available data is presented.}},
