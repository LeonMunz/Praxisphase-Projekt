@article{ ISI:000513479100001,
Author = {Toledo, D. and Cristofolini, I. and Molinari, A. and Arnhold, V. and
   Kruzhanov, V. and Vervoort, P. and Dougan, M. and Wimbert, L. and
   Bonnefoy, V. and Hellein, R. and Weber, H. and Baumgaertner, F. and
   Sicre, J. and Larsson, C. and Schneider, M.},
Title = {{High temperature sintering and its effect on dimensional and geometrical
   precision and on microstructure of low alloyed steels}},
Journal = {{POWDER METALLURGY}},
Abstract = {{The effect of high-temperature sintering (1180 degrees C and 1250
   degrees C) on the dimensional stability, geometrical precision, density
   and microstructure of rings made of five different low alloy steels was
   studied. Density and dimensional shrinkage slightly increase with
   temperature, but the dimensional and the geometrical precision of parts,
   do not depend on sintering temperature. Sintering temperature maybe
   therefore increased up to 1250 degrees C without impairing the
   dimensional and geometrical precision of the investigated rings. An
   estimation of the effect of the high sintering temperature on tensile
   properties is presented, based on the microstructure and on the fraction
   of the load-bearing section. A significant increase in both tensile
   strength and tensile ductility may be expected, in particular when the
   sintering temperature is increased up to 1250 degrees C.}},
