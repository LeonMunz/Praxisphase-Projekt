@article{ ISI:000517524200001,
Author = {Lang, Philipp A. and Crome, Sarah Q. and Xu, Haifeng C. and Lang, Karl
   S. and Chapatte, Laurence and Deenick, Elissa K. and Grusdat, Melanie
   and Pandyra, Aleksandra A. and Pozdeev, I, Vitaly and Wang, Ruifeng and
   Holderried, Tobias A. W. and Cantor, Harvey and Diefenbach, Andreas and
   Elford, Alisha R. and McIlwain, David R. and Recher, Mike and
   Haeussinger, Dieter and Mak, Tak W. and Ohashi, Pamela S.},
Title = {{NK Cells Regulate CD8(+) T Cell Mediated Autoimmunity}},
Journal = {{FRONTIERS IN CELLULAR AND INFECTION MICROBIOLOGY}},
Year = {{2020}},
Volume = {{10}},
Month = {{FEB 13}},
Abstract = {{Elucidating key factors that regulate immune-mediated pathology in vivo
   is critical for developing improved strategies to treat autoimmune
   disease and cancer. NK cells can exhibit regulatory functions against
   CD8(+) T cells following viral infection. Here we show that while low
   doses of lymphocytic choriomeningitis virus (LCMV-WE) can readily induce
   strong CD8(+) T cell responses and diabetes in mice expressing the LCMV
   glycoprotein on beta-islet cells (RIP-GP mice), hyperglycemia does not
   occur after infection with higher doses of LCMV. High-dose LCMV
   infection induced an impaired CD8(+) T cell response, which coincided
   with increased NK cell activity during early time points following
   infection. Notably, we observed increased NKp46 expression on NK cells
   during infection with higher doses, which resulted in an NK cell
   dependent suppression of T cells. Accordingly, depletion with antibodies
   specific for NK1.1 as well as NKp46 deficiency (Ncr1(gfp/gfp) mice)
   could restore CD8(+) T cell immunity and permitted the induction of
   diabetes even following infection of RIP-GP mice with high-dose LCMV.
   Therefore, we identify conditions where innate lymphoid cells can play a
   regulatory role and interfere with CD8(+) T cell mediated tissue
   specific pathology using an NKp46 dependent mechanism.}},
