@article{ ISI:000508560000058,
Author = {Reinsberg, Philip Heinrich and Baltruschat, Helmut},
Title = {{Potential- and cation-dependent adsorption of acetonitrile on gold
   investigated via surface enhanced infrared absorption spectroscopy}},
Journal = {{ELECTROCHIMICA ACTA}},
Year = {{2020}},
Volume = {{334}},
Month = {{FEB 20}},
Abstract = {{Non-aqueous electrolytes become increasingly important in modern
   electrochemical applications. In this work, we explore the adsorption of
   acetonitrile thin-film gold electrodes via surface enhanced infrared
   absorption spectroscopy (SEIRAS) in the presence of the perchlorates of
   Li, Na, Ca, Mg and tetrabutylammonium. At potentials negative of the
   point of zero charge, acetonitrile is adsorbed perpendicular with the
   methyl group oriented towards the electrode. This has been inferred from
   the abnormally high intensity of the CH3 stretching vibration at 2909
   cm(-1) and its relatively large Stark shift of 10 cm(-1) V-1 in the
   presence of Li+. In the CN-vibration region three different species can
   be distinguished. The most important of these species is adsorbed
   acetonitrile, which is not in contact with the cation. The Stark shifts
   of the CN vibration of this adsorbed acetonitrile molecules is around 4
   cm(-1) V-1. The two other species comprise adsorbed acetonitrile, which
   is influenced by cations in the second adsorption layer and acetonitrile
   molecules, which are not specifically adsorbed at the surface but are
   oriented perpendicular to the surface. The comparison between the
   spectra in presence of different cations shows that the effect of the
   cation on the adsorption spectra is not restricted to its charge
   density. This is in contrast to the solution spectra of acetonitrile,
   where cations tend to shift the CN-vibrations to larger wavenumbers. (C)
   2020 Elsevier Ltd. All rights reserved.}},
