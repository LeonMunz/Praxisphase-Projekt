@article{ ISI:000514748000047,
Author = {Schmidt, Christian M. and Sprunk, Matthias and Loeffler, Ronny and
   Hinrichs, Jorg},
Title = {{Relating nanofiltration membrane morphology to observed rejection of
   saccharides}},
Journal = {{SEPARATION AND PURIFICATION TECHNOLOGY}},
Year = {{2020}},
Volume = {{239}},
Month = {{MAY 15}},
Abstract = {{Nanofiltration for the fractionation of uncharged solutes has become
   more interesting in recent times, due to the goal of valorizing
   side-streams and by-products of the food industry. Since the membranes
   suitable for this purpose are mainly utilized for demineralization
   processes, the specifications given by the manufacturer are insufficient
   for the prediction of the rejection of uncharged solutes. Nine membranes
   with a molecular weight cut-off between 150 and 1000 Da were
   investigated showing differences in their rejection of fructose at a
   transmembrane pressure difference Delta p(TM) of 1.0 MPa. These values
   were correlated with the root-mean-square roughness S-q and apparent
   pore size d(ap) determined by atomic force microscopy, while the
   determined contact angle Theta did not influence the rejection. Thereby,
   a correlation between membrane morphology and rejection behavior was
   established.}},
