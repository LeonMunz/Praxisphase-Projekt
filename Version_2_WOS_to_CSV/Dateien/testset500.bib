@article{ ISI:000514638400003,
Author = {Agius, Rudi and Brieghel, Christian and Andersen, Michael A. and
   Pearson, Alexander T. and Ledergerber, Bruno and Cozzi-Lepri, Alessandro
   and Louzoun, Yoram and Andersen, Christen L. and Bergstedt, Jacob and
   von Stemann, Jakob H. and Jorgensen, Mette and Tang, Man-Hung Eric and
   Fontes, Magnus and Bahlo, Jasmin and Herling, Carmen D. and Hallek,
   Michael and Lundgren, Jens and MacPherson, Cameron Ross and Larsen, Jan
