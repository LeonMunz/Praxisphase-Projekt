@article{ ISI:000516018500005,
Author = {Allert, Niels and Jin, Wei and Toulaki, Xanthi and Coenen, Volker Arnd},
Title = {{DBS dysfunction mimicking transient ischemic attacks-a case report}},
Journal = {{ACTA NEUROCHIRURGICA}},
Abstract = {{We report on a patient with thalamic deep brain stimulation (DBS) for
   essential tremor who was admitted to a stroke unit with transient
   vertigo, dysarthria, and gait disturbance. Transient ischemic attacks
   were assumed but fluctuating neurological symptoms persisted until
   presentation to a DBS center. Here, unstable high monopolar impedances
   of the right-hemispheric electrode contacts were detected. Surgical
   revision revealed a fracture of the pocket adaptor connecting this
   electrode to the impulse generator. Replacement resulted in stable
   impedances and remitted the transient neurological symptoms. Emergency
   and stroke doctors should be aware of neurological symptoms induced by
   technical dysfunctions in DBS.}},
